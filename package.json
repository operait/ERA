{
  "name": "era-mvp",
  "version": "1.0.0",
  "description": "HR Assistant bot for Fitness Connection - Teams integration with RAG",
  "main": "dist/bot/app.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/bot/app.ts",
    "start": "node dist/bot/app.js",
    "ingest": "tsx src/ingestion/load-policies.ts",
    "embeddings": "tsx src/embeddings/generate.ts",
    "deploy-data": "node scripts/deploy-data.js",
    "check-data": "node scripts/check-data.js",
    "setup": "node scripts/setup.js",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.65.0",
    "@supabase/supabase-js": "^2.38.0",
    "@types/node": "^20.8.0",
    "@types/restify": "^8.5.10",
    "botbuilder": "^4.20.0",
    "dotenv": "^16.3.0",
    "openai": "^4.20.0",
    "restify": "^11.1.0",
    "tsx": "^3.14.0",
    "typescript": "^5.2.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.51.0",
    "jest": "^29.7.0"
  },
  "keywords": [
    "hr",
    "teams-bot",
    "rag",
    "supabase",
    "anthropic"
  ],
  "author": "ERA Team",
  "license": "MIT"
}
